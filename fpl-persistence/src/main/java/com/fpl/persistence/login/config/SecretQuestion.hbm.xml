<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class
        name="com.fpl.login.entity.SecretQuestion" table="FPL_MA_SECRET_QUESTION" lazy="false">
           <id name="id" type="java.lang.Long" column="SQ_ID">   
              <generator class="native">
<!--               		<param name="sequence">FPL_SQ_SECRET_QUESTION_ID</param> -->
              </generator>
           </id>
           <property name="descrption"   column="SQ_DESCRPTION"  type="java.lang.String"/>
        </class>
</hibernate-mapping>